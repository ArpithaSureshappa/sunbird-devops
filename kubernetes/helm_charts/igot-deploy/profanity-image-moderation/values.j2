### Default variable file for profanity_image_moderation-service ###

namespace: {{ namespace }}
imagepullsecrets: {{ imagepullsecrets }}
dockerhub: {{ docker_hub_url }}


env:
  javaoptions: {{profanity_image_moderation_java_mem_limit|default('-Xmx600m')}}

replicaCount: {{profanity_image_moderation_replicacount|default(1)}}
repository: {{ image_profanity_image_moderation_service }}
image_tag: {{image_tag}}
resources:
  requests:
    cpu: {{profanity_image_moderation_cpu_req|default('100m')}}
    memory: {{profanity_image_moderation_mem_req|default('100Mi')}}
  limits:
    cpu: {{profanity_image_moderation_cpu_limit|default('1')}}
    memory: {{profanity_image_moderation_mem_limit|default('1024Mi')}}
network:
  port: 80
  targetport: {{ service_profanity_image_moderation_port }}
strategy:
  type: RollingUpdate
  maxsurge: {{ profanity_image_moderation_maxsurge|default('25%') }}
  maxunavailable: {{ profanity_image_moderation_maxunavailable|default('25%') }}

ui_proxies_bulk_upload: {{ host_bulk_upload }}


